{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "strict": true,
    "strictNullChecks": false,
    "noImplicitThis": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "paths": {
        "*": [
            "node_modules/*"
        ]
    }
},
"include": [
    "src/**/*",
    "src/**/*.json"
]
}